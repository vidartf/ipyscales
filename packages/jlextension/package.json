{
  "name": "jupyterlab-scales",
  "version": "0.2.0",
  "description": "A JupyterLab extension for ipyscales. A widget library for scales",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "dependencies": {
    "@jupyter-widgets/base": "^1.1.5",
    "@phosphor/coreutils": "^1.3.0",
    "jupyter-scales": "^1.1.0"
  },
  "devDependencies": {
    "@phosphor/application": "^1.5.0",
    "@phosphor/widgets": "^1.5.0",
    "rimraf": "^2.6.2",
    "typescript": "~2.9.1"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "notebook"
  ],
  "jupyterlab": {
    "extension": true
  },
  "scripts": {
    "clean": "rimraf lib && rimraf dist",
    "build": "npm run build:lib && npm run build:labextension",
    "build:debug": "npm run build:lib",
    "build:lib": "tsc --project src",
    "build:labextension": "rimraf dist && mkdirp dist && cd dist && npm pack ..",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "build/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vidartf/ipyscales"
  },
  "author": "A widget library for scales",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/vidartf/ipyscales/issues"
  },
  "homepage": "https://github.com/vidartf/ipyscales"
}
